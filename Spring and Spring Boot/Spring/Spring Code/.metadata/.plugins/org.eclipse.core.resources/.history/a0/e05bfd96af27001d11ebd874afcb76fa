	public static void main(String[] args) {
		ApplicationContext context =  new ClassPathXmlApplicationContext("com.springcore/LifeCycle/app.xml");
		Samosa S1 = (Samosa) context.getBean("S1");
		System.out.println(S1);

	}

}
